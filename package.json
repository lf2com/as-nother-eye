{
  "name": "@lf2com/as-nother-eye",
  "version": "1.0.0",
  "description": "As-nother-eye is an web app for us taking photo by camera of another mobile",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "source": "src/index.html",
  "homepage": "https://lf2com.github.io/as-nother-eye/",
  "scripts": {
    "dev": "pnpm clear-cache && parcel --hmr-port 8080 -p 8080 --open",
    "dev:https": "pnpm clear-cache &&parcel --hmr-port 8443 -p 8443 --https --open",
    "clear-cache": "rm -rf .parcel-cache",
    "build": "rm -rf dist && parcel build --public-url ./",
    "lint": "eslint --ext .ts,.tsx src/",
    "test": "jest"
  },
  "keywords": [
    "camera",
    "photo"
  ],
  "author": "Wan Wan",
  "license": "MIT",
  "devDependencies": {
    "@savvywombat/tailwindcss-grid-areas": "^4.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^4.0.2",
    "parcel": "^2.12.0",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-sort-json": "^4.0.0",
    "process": "^0.11.10",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
